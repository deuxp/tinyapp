<!-- 

Template to display all the URLs and 
Their shortened forms.

 -->

 <!DOCTYPE html>
 <html lang="en">
 <head>
   <%- include('./partials/head') %> 
 </head>

 <header>
   <%- include('./partials/header') %> 
 </header>
 
 <body>

  <main style="margin: 1em;">
    <h3>My URLs</h3>
    <table class="table">
      <thead>
        <tr>
          <th scope="col">Short URL</th>
          <th scope="col">Long URL</th>
        </tr>
      </thead>
      <tbody>
        <% for(let url in urls) { %>
          <tr>
            <td><%= url %></td>
            <td><%= urls[url] %></td>
          </tr>
        <% } %>
      </tbody>
    </table>
    <div class="newLink">
      <p>
        <a href="/urls/new">Create a new shortURL</a>
      </p>
    </div>
  </main>

<!-- Bootstrap JS -->
<%- include('./partials/bootstrapJS') %>
  
  
 </body>
 </html>